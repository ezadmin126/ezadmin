<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">

<urlrewrite>

    <rule>
        <from>/blog/detail-(\d+)-(\d+).html</from>
        <to>/blog/detail.html?blogId=$1&amp;currentPage=$2</to>
    </rule>
    <rule>
        <from>/blog/search-(\d+)-(\d+).html</from>
        <to>/blog/search.html?categoryId=$1&amp;currentPage=$2</to>
    </rule>
    <rule>
        <from>/blog/about.html</from>
        <to>/blog/detail.html?blogId=1&amp;currentPage=1</to>
    </rule>
    <rule>
        <from>/robots.txt</from>
        <to>/static/robots.txt</to>
    </rule>

</urlrewrite>